{
  "$schema": "https://json.schemastore.org/foundryvtt-module-manifest.json",
  "id": "my-homebrew",
  "title": "My Homebrew",
  "description": "A collection of my homebrew content.",
  "version": "1.3.2",
  "authors": [
    {
      "name": "Cyrus Leung",
      "discord": "darklight#8335",
      "url": "https://github.com/DarkLight1337"
    }
  ],
  "compatibility": {
    "minimum": "12",
    "verified": "12.331"
  },
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
        "compatibility": {
          "minimum": "4.1.0",
          "maximum": "4.9.99",
          "verified": "4.1.1"
        }
      }
    ],
    "requires": [
      {
        "id": "dnd-players-handbook",
        "type": "module",
        "manifest": "https://r2.foundryvtt.com/packages-public/dnd-players-handbook/module.json",
        "compatibility": {
          "minimum": "1.2.0",
          "verified": "1.2.0"
        }
      },
      {
        "id": "dnd-dungeon-masters-guide",
        "type": "module",
        "manifest": "https://r2.foundryvtt.com/packages-public/dnd-dungeon-masters-guide/module.json",
        "compatibility": {
          "minimum": "1.0.0",
          "verified": "1.0.0"
        }
      },
      {
        "id": "midi-qol",
        "type": "module",
        "manifest": "https://gitlab.com/tposney/midi-qol/raw/v12dnd4/package/module.json",
        "compatibility": {
          "minimum": "12.4",
          "verified": "12.4.8"
        }
      },
      {
        "id": "dae",
        "type": "module",
        "manifest": "https://gitlab.com/tposney/dae/raw/dndv4/package/module.json",
        "compatibility": {
          "minimum": "12.0.2",
          "verified": "12.0.3"
        }
      },
      {
        "id": "times-up",
        "type": "module",
        "manifest": "https://gitlab.com/tposney/times-up/raw/v11/package/module.json",
        "compatibility": {
          "minimum": "11.3",
          "verified": "11.3.19"
        }
      },
      {
        "id": "ActiveAuras",
        "type": "module",
        "manifest": "https://github.com/kandashi/Active-Auras/releases/latest/download/module.json",
        "compatibility": {
          "minimum": "0.9.14",
          "verified": "0.9.15"
        }
      }
    ]
  },
  "esmodules": [
    "src/index.mjs"
  ],
  "packs": [
    {
      "name": "equipment",
      "label": "Equipment",
      "path": "./packs/equipment",
      "type": "Item",
      "system": "dnd5e"
    },
    {
      "name": "features",
      "label": "Features",
      "path": "./packs/features",
      "type": "Item",
      "system": "dnd5e"
    },
    {
      "name": "rolltables",
      "label": "Roll Tables",
      "path": "./packs/rolltables",
      "type": "RollTable",
      "system": "dnd5e"
    }
  ],
  "packFolders": [
    {
      "name": "My Homebrew",
      "sorting": "m",
      "packs": ["equipment", "features", "rolltables"]
    }
  ],
  "url": "https://github.com/DarkLight1337/my-homebrew"
}