{
    "exclude": ["build/**", "dist/**"],
    "compilerOptions": {
        "module": "NodeNext",
        "target": "ESNext",
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "maxNodeModuleJsDepth": 8,
        "strict": true,
        "noEmit": true,
    }
}
