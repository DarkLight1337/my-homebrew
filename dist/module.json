{
    "$schema": "https://json.schemastore.org/foundryvtt-module-manifest.json",
    "id": "my-homebrew",
    "title": "My Homebrew",
    "description": "A collection of my homebrew content.",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Cyrus Leung",
            "discord": "darklight#8335",
            "url": "https://github.com/DarkLight1337"
        }
    ],
    "compatibility": {
        "minimum": "11",
        "verified": "11.315"
    },
    "relationships": {
        "systems": [
            {
                "id": "dnd5e",
                "type": "system",
                "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
                "compatibility": {
                    "minimum": "2.4.0",
                    "verified": "2.4.0"
                }
            }
        ],
        "requires": [
            {
                "id": "midi-qol",
                "type": "module",
                "manifest": "https://gitlab.com/tposney/midi-qol/raw/v11/package/module.json",
                "compatibility": {
                    "minimum": "11",
                    "verified": "11.2.4"
                }
            },
            {
                "id": "dae",
                "type": "module",
                "manifest": "https://gitlab.com/tposney/dae/raw/v11/package/module.json",
                "compatibility": {
                    "minimum": "11",
                    "verified": "11.1.4"
                }
            },
            {
                "id": "times-up",
                "type": "module",
                "manifest": "https://gitlab.com/tposney/times-up/raw/v11/package/module.json",
                "compatibility": {
                    "minimum": "11",
                    "verified": "11.1.1"
                }
            },
            {
                "id": "ActiveAuras",
                "type": "module",
                "manifest": "https://github.com/kandashi/Active-Auras/releases/latest/download/module.json",
                "compatibility": {
                    "minimum": "0.8",
                    "verified": "0.9.2"
                }
            },
            {
                "id": "itemacro",
                "type": "module",
                "manifest": "https://github.com/Foundry-Workshop/Item-Macro/releases/latest/download/module.json",
                "compatibility": {
                    "minimum": "1.10",
                    "verified": "1.10.3"
                }
            }
        ]
    },
    "esmodules": [
        "src/index.mjs"
    ],
    "packs": [
        {
            "name": "my-homebrew",
            "label": "My Homebrew",
            "path": "./packs/my-homebrew",
            "type": "Item",
            "system": "dnd5e"
        }
    ],
    "url": "https://github.com/DarkLight1337/my-homebrew",
    "manifest": "https://raw.githubusercontent.com/DarkLight1337/my-homebrew/master/dist/module.json",
    "download": "https://raw.githubusercontent.com/DarkLight1337/my-homebrew/master/dist/module.zip"
}