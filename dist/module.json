{
    "$schema": "https://json.schemastore.org/foundryvtt-module-manifest.json",
    "id": "my-homebrew",
    "title": "My Homebrew",
    "description": "A collection of my homebrew content.",
    "version": "1.4.0",
    "authors": [
        {
            "name": "Cyrus Leung",
            "discord": "darklight#8335",
            "url": "https://github.com/DarkLight1337"
        }
    ],
    "compatibility": {
        "minimum": "13",
        "verified": "13.348"
    },
    "relationships": {
        "systems": [
            {
                "id": "dnd5e",
                "type": "system",
                "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
                "compatibility": {
                    "minimum": "5.1",
                    "verified": "5.1.10"
                }
            }
        ],
        "requires": [
            {
                "id": "dnd-players-handbook",
                "type": "module",
                "manifest": "https://r2.foundryvtt.com/packages-public/dnd-players-handbook/module.json",
                "compatibility": {
                    "minimum": "2.0",
                    "verified": "2.1.0"
                }
            },
            {
                "id": "dnd-dungeon-masters-guide",
                "type": "module",
                "manifest": "https://r2.foundryvtt.com/packages-public/dnd-dungeon-masters-guide/module.json",
                "compatibility": {
                    "minimum": "2.0",
                    "verified": "2.0.0"
                }
            },
            {
                "id": "midi-qol",
                "type": "module",
                "manifest": "https://gitlab.com/tposney/midi-qol/raw/v13/package/module.json",
                "compatibility": {
                    "minimum": "13.0",
                    "verified": "13.0.28"
                }
            },
            {
                "id": "dae",
                "type": "module",
                "manifest": "https://gitlab.com/tposney/dae/raw/v13/package/module.json",
                "compatibility": {
                    "minimum": "13.0",
                    "verified": "13.0.16"
                }
            },
            {
                "id": "times-up",
                "type": "module",
                "manifest": "https://gitlab.com/tposney/times-up/raw/master/package/module.json",
                "compatibility": {
                    "minimum": "13.1",
                    "verified": "13.1.0"
                }
            },
            {
                "id": "ActiveAuras",
                "type": "module",
                "manifest": "https://github.com/kandashi/Active-Auras/releases/latest/download/module.json",
                "compatibility": {
                    "minimum": "0.12",
                    "verified": "0.12.4"
                }
            }
        ]
    },
    "esmodules": [
        "src/index.mjs"
    ],
    "packs": [
        {
            "name": "equipment",
            "label": "Equipment",
            "path": "./packs/equipment",
            "type": "Item",
            "system": "dnd5e"
        },
        {
            "name": "features",
            "label": "Features",
            "path": "./packs/features",
            "type": "Item",
            "system": "dnd5e"
        },
        {
            "name": "rolltables",
            "label": "Roll Tables",
            "path": "./packs/rolltables",
            "type": "RollTable",
            "system": "dnd5e"
        }
    ],
    "packFolders": [
        {
            "name": "My Homebrew",
            "sorting": "m",
            "packs": [
                "equipment",
                "features",
                "rolltables"
            ]
        }
    ],
    "url": "https://github.com/DarkLight1337/my-homebrew",
    "manifest": "https://raw.githubusercontent.com/DarkLight1337/my-homebrew/master/dist/module.json",
    "download": "https://raw.githubusercontent.com/DarkLight1337/my-homebrew/master/dist/module.zip"
}